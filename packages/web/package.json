{
  "name": "@devon/web",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "build": "tsc -b && react-app-rewired build && rm -rf dist && mv build dist",
    "clean": "rm -rf dist/*",
    "eject": "react-app-rewired eject",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "now-build": "build",
    "predeploy": "npm run build",
    "prepare": "cd .. && yarn patch-package",
    "serve": "serve dist",
    "dev": "react-app-rewired start",
    "test": "echo 'web will be tested soon!'"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@devon/components": "0.0.1",
    "react": "16.7.0-alpha.2",
    "react-app-polyfill": "0.2.0",
    "react-art": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-native-web": "0.9.10",
    "react-scripts": "2.1.1",
    "resize-observer-polyfill": "1.5.1",
    "smoothscroll-polyfill": "0.4.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.9",
    "@types/node": "10.12.24",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "babel-plugin-react-native-web": "0.10.0",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.16.4",
    "react-app-rewired": "2.1.0",
    "serve": "10.1.2",
    "tslint": "5.13.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3333",
    "webpack-bundle-analyzer": "3.0.4"
  },
  "resolutions": {
    "scheduler": "0.12.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ]
}
